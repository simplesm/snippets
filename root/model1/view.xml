<mvc:View controllerName="com.simple.sm.snippets.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
          <Text class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" text="{MyJSONModel>/description}"/>
          <Text class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" text="{MyJSONModel>/data}"/>					
        </content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
